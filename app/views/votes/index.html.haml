%h1 Listing votes

%table
  %tr
    %th Precicnt
    %th Candidate
    %th Votes
    %th
    - if user_signed_in?
      %th
      %th

  - @votes.each do |vote|
    %tr
      %td= vote.precinct.number
      %td= vote.candidate.name
      %td= vote.votes
      %td= link_to 'Show', vote
      - if user_signed_in?
        %td= link_to 'Edit', edit_vote_path(vote)
        %td= link_to 'Destroy', vote, :confirm => 'Are you sure?', :method => :delete

%br
- if user_signed_in?
  = link_to 'New Vote', new_vote_path
